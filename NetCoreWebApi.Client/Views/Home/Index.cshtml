@using NetCoreWebApi.Models.Concrete;
@model IEnumerable<CategoryDTO>
@{
    ViewData["Title"] = "Home Page";
}

category
<hr />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td> @item.Id </td>
                <td>@item.Name </td>
            </tr>
        }
    </tbody>
</table>